<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">
           New Todo
        </h3>
    </div>
    <div class="panel-body">
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @todo.name %>
</p>

<p>
  <strong>Note:</strong>
  <%= @todo.notes %>
</p>



<%= link_to 'Edit', edit_todo_path(@todo) %> |
<%= link_to 'Back', todos_path %>
</div>
</div>
